{
    "workbench.productIconTheme": "material-product-icons",
    "workbench.iconTheme": "material-icon-theme",
    "telemetry.telemetryLevel": "off",
    "workbench.panel.defaultLocation": "right",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font Mono'",
    "editor.fontLigatures": true,
    "editor.wordWrap": "on",
    "editor.rulers": [
            72,
            79,
            99
        ],
    "platformio-ide.disablePIOHomeStartup": true,
    "files.autoSave": "onWindowChange",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "diffEditor.codeLens": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.enableExperiments": false,
    "workbench.experimental.editor.dropIntoEditor.enabled": false,
    "workbench.preferredHighContrastColorTheme": "Andromeda Colorizer",
    "window.confirmBeforeClose": "always",
    "debug.confirmOnExit": "always",
    "notebook.lineNumbers": "on",
    "C_Cpp.autocompleteAddParentheses": true,
    "python.languageServer": "Pylance",
    "workbench.preferredDarkColorTheme": "Andromeda Colorizer",
    "editor.fontWeight": "normal",
    "editor.guides.bracketPairs": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.detectIndentation": false,
    "editor.linkedEditing": true,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "workbench.preferredHighContrastLightColorTheme": "Andromeda Colorizer",
    "workbench.preferredLightColorTheme": "Andromeda Colorizer",
    "explorer.fileNesting.enabled": true,
    "terminal.integrated.confirmOnExit": "always",
    "terminal.integrated.cursorBlinking": true,
    "update.mode": "none",
    "jupyter.askForKernelRestart": false,
    "better-comments.tags": [
            {
                "tag": "!",
                "color": "#FF2D00",
                "strikethrough": false,
                "underline": false,
                "backgroundColor": "transparent",
                "bold": false,
                "italic": false
            },
            {
                "tag": "?",
                "color": "#3498DB",
                "strikethrough": false,
                "underline": false,
                "backgroundColor": "transparent",
                "bold": false,
                "italic": false
            },
            {
                "tag": "~~",
                "color": "#474747",
                "strikethrough": true,
                "underline": false,
                "backgroundColor": "transparent",
                "bold": false,
                "italic": false
            },
            {
                "tag": "todo",
                "color": "#FF8C00",
                "strikethrough": false,
                "underline": false,
                "backgroundColor": "transparent",
                "bold": false,
                "italic": false
            },
            {
                "tag": "*",
                "color": "#98C379",
                "strikethrough": false,
                "underline": false,
                "backgroundColor": "transparent",
                "bold": false,
                "italic": false
            }
        ],
    "extensions.ignoreRecommendations": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.showReleaseNotes": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "npm.fetchOnlinePackageInfo": false,
    "editor.foldingStrategy": "indentation",
    "editor.links": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.unfoldOnClickAfterEndOfLine": true,
    "editor.unicodeHighlight.includeComments": true,
    "editor.unicodeHighlight.nonBasicASCII": true,
    "diffEditor.maxFileSize": 0,
    "editor.minimap.renderCharacters": false,
    "editor.inlineSuggest.enabled": false,
    "editor.snippetSuggestions": "none",
    "editor.suggest.preview": true,
    "editor.suggest.showDeprecated": false,
    "editor.wordBasedSuggestionsMode": "currentDocument",
    "files.eol": "\n",
    "files.maxMemoryForLargeFilesMB": 8192,
    "files.hotExit": "off",
    "workbench.commandPalette.history": 0,
    "workbench.panel.opensMaximized": "never",
    "workbench.reduceMotion": "on",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.editor.autoLockGroups": {
            "default": true,
            "terminalEditor": false
        },
    "workbench.editor.historyBasedLanguageDetection": false,
    "workbench.editor.languageDetection": false,
    "workbench.editor.languageDetectionHints": {
            "untitledEditors": false,
            "notebookEditors": false
        },
    "workbench.editor.limit.value": 4,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.restoreViewState": false,
    "window.autoDetectHighContrast": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "terminal.explorerKind": "external",
    "terminal.integrated.enablePersistentSessions": false,
    "problems.autoReveal": false,
    "problems.defaultViewMode": "table",
    "debug.console.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "editor.lineHeight": 1.4,
    "markdown.preview.lineHeight": 1.4,
    "editor.fontSize": 16,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Andromeda Colorizer",
    "gtkTitleBar.mode": "dark",

}
