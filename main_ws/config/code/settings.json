    "editor.scrollbar.horizontal": "visible",
    "diffEditor.wordWrap": "on",
    "editor.suggest.preview": true,
    "editor.suggest.showOperators": false,
    "editor.suggest.showUsers": false,
    "editor.suggest.showValues": false,
    "editor.suggest.showWords": false,
    "workbench.tree.indent": 12,
    "window.titleSeparator": " || ",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "window.titleBarStyle": "custom",
    "workbench.iconTheme": "material-icon-theme",
    "notebook.output.fontFamily": "'Cascadia Code'",
    "window.zoomLevel": 0.75,
    "editor.inlineSuggest.fontFamily": "'Cascadia Code'",
    "chat.editor.fontFamily": "'Cascadia Code'",
    "editor.experimentalInlineEdit.fontFamily": "'Cascadia Code'",
    "terminal.integrated.fontWeight": "600",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.experiments.enabled": false,
    "jupyter.enableExtendedPythonKernelCompletions": true,
