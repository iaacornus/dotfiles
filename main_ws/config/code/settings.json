{
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "editor.fontFamily": "'Cascadia Code'",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.4,
    "editor.codeLensFontFamily": "'Cascadia Code'",
    "editor.inlayHints.fontFamily": "'Cascadia Code'",
    "debug.console.fontFamily": "'Cascadia Code'",
    "debug.console.fontSize": 16,
    "debug.console.lineHeight": 1.4,
    "terminal.integrated.fontFamily": "'Cascadia Code'",
    "terminal.integrated.fontSize": 16,
    "markdown.preview.fontFamily": "'Cantarell'",
    "markdown.preview.lineHeight": 1.2,
    "scm.inputFontFamily": "'Cantarell'",
    "scm.inputFontSize": 12,
    "workbench.panel.defaultLocation": "right",
    "editor.wordWrap": "on",
    "editor.rulers": [
            58,
            72,
            79
        ],
