{
    "redhat.telemetry.enabled": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font'",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss19', 'ss20', 'calt'",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.4,
    "editor.codeLensFontFamily": "'CaskaydiaCove Nerd Font'",
    "editor.inlayHints.fontFamily": "'CaskaydiaCove Nerd Font'",
    "debug.console.fontFamily": "'CaskaydiaCove Nerd Font'",
    "debug.console.fontSize": 16,
    "debug.console.lineHeight": 1.4,
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font'",
    "terminal.integrated.fontSize": 16,
    "markdown.preview.fontFamily": "'Noto Sans'",
    "markdown.preview.lineHeight": 1.2,
    "scm.inputFontFamily": "'Noto Sans'",
    "scm.inputFontSize": 12,
    "workbench.panel.defaultLocation": "right",
    "editor.wordWrap": "on",
    "editor.rulers": [
            58,
            72,
            79
        ],
    "editor.semanticHighlighting.enabled": true,
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.nonBasicASCII": true,
    "editor.foldingStrategy": "indentation",
    "editor.showFoldingControls": "always",
    "editor.renderLineHighlight": "all",
    "editor.scrollbar.vertical": "visible",
    "editor.guides.bracketPairs": true,
    "editor.minimap.showSlider": "always",
    "editor.showUnused": true,
    "explorer.fileNesting.enabled": true,
    "notebook.lineNumbers": "on",
    "notebook.showFoldingControls": "always",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.unfoldOnClickAfterEndOfLine": true,
    "files.insertFinalNewline": true,
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "workbench.editor.historyBasedLanguageDetection": false,
    "workbench.editor.languageDetection": false,
    "workbench.editor.autoLockGroups": {
        "default": true,
        "terminalEditor": false
    },
    "workbench.editor.languageDetectionHints": {
            "untitledEditors": false,
            "notebookEditors": false
        },
    "workbench.localHistory.maxFileEntries": 5,
    "editor.linkedEditing": true,
    "emmet.showSuggestionsAsSnippets": true,
    "debug.console.collapseIdenticalLines": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.explorerKind": "external",
    "diffEditor.maxFileSize": 0,
    "files.eol": "\n",
    "workbench.commandPalette.history": 8,
    "workbench.panel.opensMaximized": "never",
    "workbench.reduceMotion": "on",
    "extensions.ignoreRecommendations": true,
    "editor.accessibilitySupport": "off",
    "editor.find.cursorMoveOnType": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.suggest.showCustomcolors": false,
    "editor.suggest.showIssues": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.comments.ignoreEmptyLines": false,
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.lightbulb.enabled": "off",
    "editor.links": false,
    "editor.rename.enablePreview": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.tips.enabled": false,
    "workbench.editor.restoreViewState": false,
    "workbench.welcomePage.walkthroughs.openOnInstall": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.localHistory.enabled": false,
    "emmet.showAbbreviationSuggestions": false,
    "npm.fetchOnlinePackageInfo": false,
    "git.suggestSmartCommit": false,
    "jupyter.experiments.enabled": false,
    "jupyter.askForKernelRestart": false,
    "terminal.integrated.enablePersistentSessions": false,
    "problems.autoReveal": false,
    "debug.confirmOnExit": "always",
    "terminal.integrated.confirmOnExit": "always",
    "zenMode.hideActivityBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.restore": false,
    "scm.alwaysShowRepositories": true,
    "python.languageServer": "Pylance",
    "markdown.preview.fontSize": 16,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.guides.highlightActiveIndentation": "always",
    "editor.inlayHints.enabled": "off",
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.scrollbar.horizontal": "visible",
    "diffEditor.wordWrap": "on",
    "editor.suggest.preview": true,
    "editor.suggest.showOperators": false,
    "editor.suggest.showUsers": false,
    "editor.suggest.showValues": false,
    "editor.suggest.showWords": false,
    "workbench.tree.indent": 12,
    "window.titleSeparator": " || ",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "workbench.iconTheme": "material-icon-theme",
    "notebook.output.fontFamily": "'CaskaydiaCove Nerd Font'",
    "window.zoomLevel": 0.75,
    "editor.inlineSuggest.fontFamily": "'CaskaydiaCove Nerd Font'",
    "chat.editor.fontFamily": "'CaskaydiaCove Nerd Font'",
    "editor.experimentalInlineEdit.fontFamily": "'CaskaydiaCove Nerd Font'",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.experiments.enabled": false,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "standard",
    "workbench.productIconTheme": "material-product-icons",
    "workbench.preferredHighContrastColorTheme": "Sublime Material Theme - Dark",
    "workbench.preferredHighContrastLightColorTheme": "Julia Light",
    "workbench.preferredDarkColorTheme": "Sublime Material Theme - Dark",
    "workbench.preferredLightColorTheme": "Julia Light",
    "workbench.colorTheme": "Sublime Material Theme - Dark",
    "editor.lineNumbers": "relative",
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false
    },
    "diffEditor.hideUnchangedRegions.enabled": true,
    "better-comments.highlightPlainText": true,
    "workbench.editorAssociations": {
        "file:/**/*.csv": "jupyter-data-wrangler"
    },
    "presentation-mode.active": {
        "commands": [
            "workbench.action.closeSidebar"
        ],
        "editor.fontSize": 18,
        "editor.matchBrackets": "never",
        "editor.minimap.enabled": false,
        "editor.scrollbar.verticalScrollbarSize": 0,
        "workbench.activityBar.visible": false,
        "workbench.statusBar.visible": false,
        "window.zoomLevel": 2
    },
    "presentation-mode.inactive": {
        "commands": [
            "workbench.action.focusSideBar"
        ]
    },
    "presentation-mode.configBackup": {
        "editor.fontSize": "undefined",
        "editor.matchBrackets": "undefined",
        "editor.minimap.enabled": "undefined",
        "editor.scrollbar.verticalScrollbarSize": "undefined",
        "workbench.activityBar.visible": "undefined",
        "workbench.statusBar.visible": "undefined",
        "window.zoomLevel": "undefined"
    },
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": true,
    "C_Cpp.vcFormat.indent.braces": true,
    "C_Cpp.vcFormat.space.betweenEmptyBraces": true,
    "C_Cpp.vcFormat.space.beforeLambdaOpenParenthesis": true,
    "C_Cpp.vcFormat.space.beforeInitializerListOpenBrace": true,
    "C_Cpp.vcFormat.space.beforeOpenSquareBracket": true,
    "C_Cpp.vcFormat.space.insertAfterSemicolon": true,
}
